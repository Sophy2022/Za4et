@import url(http://fonts.googleapis.com/css?family=Quattrocento+Sans-Bold|Quattrocento);
@import url(http://fonts.googleapis.com/css?family=Montserrat|Montserrat);


body {
    background-color: #ffebcd;
    margin: 0;
	
}
header {
    background-color: #878787;
    height: 120px;
	
}
	

.content {
	position: fixed;
	bottom: 45%;
	right: calc((90vw - 1000px) / 2);
	
}
footer {
    background-color: #878787;
    position: fixed;
    bottom: 0px;
    width: 100%;
}
.wrapper {
    width: 980px;
    margin: auto;
	
}


header h1 {
    font-family: 
	'Quattrocento Sans',
	sans-serif;
    font-weight: 900;
    text-align: center;
    padding-top: 10px;
    padding-bottom: 0px;
    color: #ffebcd;
	line-height: 1;
	
}
	header h3 {
    font-family: 
	Montserrat, Verdana, 
	Geneva,Tahoma, 
	sans-serif;
    text-align: center;
    padding-top: 0px;
    color: #ffebcd;
}


.round {
    position: fixed;
	bottom: 50%;
	left: 10%;
	right: 50%;
	height:300px;
	width: 300px; 
	border-radius: 60%;
	
		
}
h2 {
    font-family: 'Quattrocento Sans', sans-serif;
	display: flex;
	align-self: center;
	flex-direction:row;
	text-align:center;
	margin-bottom: 30px;
	margin-top: -23%;
	padding-right:25%;
    color: #264653;
	line-height:1;
}
p {
    font-family: 
	Montserrat, Verdana, 
	Geneva, Tahoma,
	sans-serif;
    color: #264653; 
	line-height: 1;
	text-align:center;
	display: flex;
	flex-direction:row;
}
li {
    font-family:
	Montserrat, Verdana, 
	Geneva, Tahoma, 
	sans-serif;
    color: #264653;
	line-height: 1.5;
}
footer p {
    color: #ffebcd;
}
#copyright {
	display:flex;
	flex-direction:row;
	justify-content:center;
}

a:visited {
	color: #8a2be2;
	}
	
.mylink:hover{
	color: blue;
	background: red;
	}
	
	a {text-decoration: none;
	}



	





